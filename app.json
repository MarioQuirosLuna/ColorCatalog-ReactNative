{
  "expo": {
    "name": "ColorCatalog-ReactNative",
    "description": "Catalog your colors. Save your favorite wen colors in a list and view the details for each color.",
    "slug": "ColorCatalog-ReactNative",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/cc-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/cc-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    }
  }
}